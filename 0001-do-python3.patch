 if(NOT PYTHON)
 	message(FATAL_ERROR "You need python to build ${PROJECT_NAME}")
 endif(NOT PYTHON)
diff --git a/src/addressbook/libebook-contacts/gen-western-table.py b/src/addressbook/libebook-contacts/gen-western-table.py
